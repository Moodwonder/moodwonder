{
    "rules": {
        "quotes": [1, "single"],
        "no-unused-vars": 1,
        "strict": 0,
        "new-cap": [2, { "capIsNewExceptions": ["Map"] }]
    },
    "env":{
        "es6": true
    },
    "ecmaFeatures": {
        "modules": true,
        "blockBindings": true,
        "jsx": true
    }
}
